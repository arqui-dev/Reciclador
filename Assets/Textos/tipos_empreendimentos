		valor venda: 2n
		experiencia: 10n*N[j]
		dinheiro gerado: n
		separacao automatica: n
		preco: (N[j]+100+t)*((Base$^n)
		nivel sustentabilidade	: N[6,18,48,60] + 2t
		empreendimentos construidos: catadores_TIPO : n+1 ;
